{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Class Room Bind Suggest API Type Schema",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "updated": {
            "type": "string"
        }
    },
    "patternProperties": {
        "^//.*$": {}
    },
    "additionalProperties": {
